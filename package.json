{
  "name": "prisma-cms",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "prettier-eslint-cli": "4.7.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "@types/react": "^16.1.0",
    "apollo-boost": "^0.1.16",
    "apollo-cache-inmemory": "^1.2.10",
    "apollo-client": "^2.4.2",
    "apollo-error": "^1.0.2",
    "apollo-fetch-upload": "^1.2.0",
    "apollo-link": "^1.2.3",
    "apollo-link-batch-http": "^1.0.4",
    "apollo-link-error": "^1.1.1",
    "apollo-link-http": "1.3.2",
    "apollo-link-ws": "^1.0.9",
    "apollo-upload-client": "^9.0.0",
    "apollo-utilities": "^1.0.21",
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-0": "^6.24.1",
    "debug": "^3.1.0",
    "dom-scroll-into-view": "^1.2.1",
    "draft-js": "^0.10.5",
    "graphql": "^0.13.2",
    "graphql-cli": "^2.15.13",
    "graphql-tag": "2.6.1",
    "jss-theme-reactor": "^0.11.1",
    "load-script": "^1.0.0",
    "material-ui": "1.0.0-beta.43",
    "material-ui-icons": "^1.0.0-beta.17",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "node-less-chokidar": "^0.1.2",
    "node-sass-chokidar": "^1.1.0",
    "prisma-binding": "2.1.5",
    "react": "^16.5.2",
    "react-apollo": "^2.2.3",
    "react-dom": "^16.5.2",
    "react-router-dom": "4.2.2",
    "react-scripts": "^2.0.2",
    "shortid": "^2.2.8",
    "socket.io": "^2.0.4",
    "socket.io-parser": "^3.1.2",
    "subscriptions-transport-ws": "^0.9.5",
    "urijs": "^1.19.1",
    "xml-writer": "^1.7.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "start-ssr": "NODE_ENV=production node ssr",
    "start-server": "Sendmail=true action=start-server nodemon src/index.js",
    "start-server-dev": "Sendmail=true SendmailTest=true action=start-server yarn start",
    "deploy": "run-s build-schema-prisma && yarn deploy-schema && yarn get-schema -p prisma && yarn build-schema-api",
    "deploy-force": "run-s build-schema-prisma && yarn deploy-schema -f && yarn get-schema -p prisma && yarn build-schema-api",
    "deploy-prod": "yarn deploy",
    "build-schema-prisma": "schemaType=prisma yarn build-schema",
    "deploy-schema": "node_modules/prisma/dist/index.js deploy",
    "get-schema": "node_modules/graphql-cli/dist/bin.js get-schema -p prisma -o src/schema/generated/prisma.graphql",
    "build-schema-api": "schemaType=api yarn build-schema",
    "build-schema": "action=build-schema node server/index.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
